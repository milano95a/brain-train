/*** redirects.js ***/
(function(){var alternateLinks=document.head.querySelectorAll('link[rel="alternate"]');var alternateLink=Array.prototype.filter.call(alternateLinks,function(el){return el.media;})[0];if(alternateLink){var mediaQuery=alternateLink.getAttribute("media");var matchMediaQuery=window.matchMedia(mediaQuery).matches;var currentPage=window.location.host.split(".");var isMobile=currentPage.some(function(el){return(el==="m");});var rel=isMobile?"canonical":"alternate";var link=isMobile?document.head.querySelector('link[rel="'+rel+'"]'):alternateLink;if(link&&((matchMediaQuery&&!isMobile)||(!matchMediaQuery&&isMobile))){if(window.stop){window.stop();}window.location.replace(link.getAttribute("href"));}}}());
